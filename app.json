{
  "expo": {
    "name": "YES MECHANIC",
    "slug": "smsreactnativeapp",
    "version": "1.0.0",
    "icon": "./src/assets/appLogoNew.png",
    "web": {
      "favicon": "./src/assets/appLogoNew.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them.",
          "cameraPermission": "The app accesses your camera to let you take photos."
        }
      ],
      "expo-secure-store"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/appLogoNew.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.suryas_1805.smsreactnativeapp"
    },
    "extra": {
      "eas": {
        "projectId": "50e4a192-2f74-4052-9d4c-2ca792da20fc"
      }
    }
  }
}
